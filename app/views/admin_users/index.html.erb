<h1>AdminUsers#index</h1>
<p>Find me in app/views/admin_users/index.html.erb</p>


<h1>会員一覧</h1>
<table>
   <thead>
   	 <tr>
   	 	<th>ID</th>
   	 	<th>氏名</th>
   	 	<th>メールアドレス</th>
   	 	<th>ステータス</th>
   	 </tr>
   	</thead>


   <% @users.each do |user| %>
   <tbody>
   	<tr>
   		<td><%= user.id %></td>
   		<td>
   			<%= link_to(admin_user_path(user), class: 'list-content')  do %>
   			<div class="content">
   			<div class="inner=left"></div>
   			<%= user.first_name %>
   		<%= user.family_name %>
   		<% end %>
   		</td>
   		<td><%= user.email %></td>
		<td><%= user.user_status %></td>

	<% end %>
	</tr>

	</tbody>
</table>
