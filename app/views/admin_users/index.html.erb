
<h1>会員一覧</h1>
<table>
   <thead>
   	 <tr>
   	 	<th>会員ID</th>
   	 	<th>氏名</th>
   	 	<th>メールアドレス</th>
   	 	<th>ステータス</th>
   	 </tr>
   	</thead>


   <% @users.each do |user| %>
   <tbody>
   	<tr>
   		<td><%= user.id %></td>
   		<td><%= link_to(admin_user_path(user), class: 'list-content')  do %>
   			<div class="content">
   			<div class="inner=left">
   			<%= user.first_name %></div>
   			<div class="inner=right">
   			<%= user.family_name %></div>
   		<% end %></td></div>
   		<td><%= user.email %></td>
		<td><%= user.user_status %></td>

	<% end %>
	</tr>

	</tbody>
</table>
